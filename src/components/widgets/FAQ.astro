---
interface Question {
  question: string;
  answer: string;
}

interface Props {
  faqs: Question[];
}

const { faqs } = Astro.props;
---

<article>
  <div>
    <ul>
      {
        faqs.map((faq, index) => (
          <li>
            <details name="FAQ">
              <summary>{faq.question}</summary>
              <p>{faq.answer}</p>
            </details>
          </li>
        ))
      }
    </ul>
  </div>
</article>
